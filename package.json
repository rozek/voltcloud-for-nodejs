  "version": "0.1.5",
  "description": "a simple VoltCloud client library for nodejs",
  "type": "module",
  "module": "./dist/voltcloud-for-nodejs.esm.js",
  "types": "./dist/voltcloud-for-nodejs.d.ts",
  "exports": {
    ".": {
      "import": "./dist/voltcloud-for-nodejs.esm.js"
