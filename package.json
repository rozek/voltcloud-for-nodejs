  "homepage": "https://github.com/rozek/voltcloud-for-nodejs#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "agadoo": "^2.0.0",
    "chai": "^4.3.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.5"
